{
    "port": "3000",
    "serveStatic": {
        "enabled": true,
        "urlPrefix": "",
        "indexPage": [
            "index.html",
            "index.htm"
        ],
        "dirPath": "./static"
    },
    "serveApi": {
        "enabled": true,
        "urlPrefix": "/api",
        "filePath": "./json-api.hbs"
    },
    "serveWebsocket": {
        "enabled": true
    },
    "serveProxy": {
        "enabled": true,
        "endpoints": [
            {
                "endpoint": "/proxy-foo",
                "target": "http://httpbin.org/get"
            },
            {
                "endpoint": "/proxy-bar",
                "target": "",
                "options": {}
            }
        ],
        "options": {}
    }
}