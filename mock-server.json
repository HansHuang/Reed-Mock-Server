{
    "port": "3000",
    "timeout": 300,
    "serveStatic": {
        "enabled": true,
        "urlPrefix": "",
        "indexPage": [
            "index.html",
            "index.htm"
        ],
        "dirPath": "./static"
    },
    "serveApi": {
        "enabled": true,
        "urlPrefix": "/api",
        "filePath": "./json-api.hbs"
    },
    "serveWebsocket": {
        "enabled": true
    },
    "serveProxy": {
        "enabled": true,
        "endpoints": [
            {
                "endpoint": "/proxy-foo",
                "target": "http://localhost:2999/api/"
            },
            {
                "endpoint": "/proxy-bar",
                "target": "http://localhost:2999",
                "options": {}
            }
        ],
        "options": {}
    },
    "customMiddleware": {
        "front": [],
        "last": []
    }
}