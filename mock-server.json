{
    "port": "3000",
    "serveStatic": {
        "enabled": true,
        "urlPrefix": "",
        "indexPage": [
            "index.html",
            "index.htm"
        ],
        "dirPath": "./static"
    },
    "serveApi": {
        "enabled": true,
        "urlPrefix": "/api",
        "filePath": "./json-api.hbs"
    },
    "serveWebsocket": {
        "enabled": true
    },
    "serveProxy": {
        "enabled": true,
        "endpoints": [
            {
                "endpoint": "/proxy-get",
                "target": "http://httpbin.org/get"
            },
            {
                "endpoint": "/proxy-post",
                "target": "http://httpbin.org/post",
                "options": {}
            }
        ],
        "options": {}
    }
}